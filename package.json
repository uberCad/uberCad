{
  "name": "client",
  "version": "1.2.3",
  "private": true,
  "repository": "git@gitlab.com:Potter/uberCad.git",
  "author": "Gary <netzver@gmail.com>",
  "dependencies": {
    "antd": "^3.26.2",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "dxf-parser": "0.5.1",
    "immutability-helper": "^3.0.1",
    "kmeans-js": "^0.1.2",
    "lodash": "^4.17.5",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.12.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.12.0",
    "react-intl": "^2.4.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.3.0",
    "stats.js": "^0.17.0",
    "three": "^0.110.0",
    "three-dxf": "^0.3.1",
    "three-orbit-controls": "^82.1.0"
  },
  "scripts": {
    "build": "npm run build:css && react-scripts build",
    "start": "npm-run-all -p watch:css start:js",
    "heroku-postbuild": "npm run build",
    "build:css": "node-sass src/ -o src/ --source-map true",
    "watch:css": "npm run build:css && node-sass -w src/ -o src/ --recursive",
    "start:js": "node_modules/.bin/react-scripts start",
    "test": "standard --verbose && babel-node ./node_modules/.bin/tape ./test/**/*.js --require ignore-styles --require ./test/setup.js | faucet",
    "fix": "standard --verbose --fix | snazzy",
    "eject": "react-scripts eject",
    "jest": "react-scripts test --env=jsdom",
    "lint": "eslint --fix src && echo 'Lint complete.'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(three|react-redux)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1"
  }
}
